@page
@model Assignment1Attempt4.Areas.Identity.Pages.UserManagement.UserInfoEditModel
@{
	ViewData["Title"] = "UserInfoEdit";
	Layout = "/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<h1>
	Profile Page: [name here]
</h1>

<body class="User Data">
	<div class="Data Labels" align="left" style="background-color:dimgray">

		<label for="fname">First Name:</label>
		<input type="text" id="fname" name="fname"><br><br>

		<label for="lname">Last Name:</label>
		<input type="text" id="lname" name="lname"><br><br>

		<label for="email">Email:</label>
		<input type="text" id="email" name="email"><br><br>

		<label for="addr1">Address 1:</label>
		<input type="text" id="addr1" name="addr1"><br><br>

		<label for="addr2">Address 2:</label>
		<input type="text" id="addr2" name="addr2"><br><br>

		<label for="city">City:</label>
		<input type="text" id="city" name="city"><br><br>

		<label for="state">State:</label>
		<input type="text" id="state" name="state"><br><br>

		<label for="zip">Zip Code:</label>
		<input type="text" id="zip" name="zip"><br><br>
	</div>

</body>


</html>